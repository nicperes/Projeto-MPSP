                    <label>Número</label>
                    <input class="input100" type="text" disabled="disabled" value="@Html.DisplayFor(model => model.Item3.Numero)">

                    <label>Complemento</label>
                    <input class="input100" type="text" disabled="disabled" value="@Html.DisplayFor(model => model.Item3.Complemento)">

                    <label>Bairro</label>
                    <input class="input100" type="text" disabled="disabled" value="@Html.DisplayFor(model => model.Item3.Bairro)">

                    <label>Municipio</label>
                    <input class="input100" type="text" disabled="disabled" value="@Html.DisplayFor(model => model.Item3.Municipio)">

                    <label>CEP</label>
                    <input class="input100" type="text" disabled="disabled" value="@Html.DisplayFor(model => model.Item3.Cep)">

                    <label>UF</label>
                    <input class="input100" type="text" disabled="disabled" value="@Html.DisplayFor(model => model.Item3.Uf)">

                    <br />

                    <span class="login100-form-title p-b-34 p-t-27">
                        Caged
                    </span>

                    <label>Telefone</label>
                    <input class="input100" type="text" disabled="disabled" value="@Html.DisplayFor(model => model.Item4.Telefone)">

                    <label>Ramal</label>
                    <input class="input100" type="text" disabled="disabled" value="@Html.DisplayFor(model => model.Item4.Ramal)">

                    <label>E-mail</label>
                    <input class="input100" type="text" disabled="disabled" value="@Html.DisplayFor(model => model.Item4.Email)">

                    <br />

                    <div class="container-login100-form-btn">
                        <button class="login100-form-btn">
                            @Html.ActionLink("Gerar PDF", "RelatorioPdf")
                        </button>
                    </div>

                    <br />

                    <div class="container-login100-form-btn">
                        <button class="login100-form-btn">
                            @Html.ActionLink("Voltar", "Menu")
                        </button>
                    </div>

                </form>
            </div>
        </div>
    </div>


    <div id="dropDownSelect1"></div>


                    <input class="input100" type="text" disabled="disabled" value="@Html.DisplayFor(model => model.Item1.NovoNomeConj2)">

                    <label>Data do Casamento</label>
                    <input class="input100" type="text" disabled="disabled" value="@Html.DisplayFor(model => model.Item1.DataCasamento)">

                    <label>Matricula</label>
                    <input class="input100" type="text" disabled="disabled" value="@Html.DisplayFor(model => model.Item1.Matricula)">

                    <label>Data da Entrada</label>
                    <input class="input100" type="text" disabled="disabled" value="@Html.DisplayFor(model => model.Item1.DataEntrada)">

                    <label>Data do Registro</label>
                    <input class="input100" type="text" disabled="disabled" value="@Html.DisplayFor(model => model.Item1.DataRegistro)">

                    <br />

                    <span class="login100-form-title p-b-34 p-t-27">
                        Cadesp
                    </span>

                    <label>CNPJ</label>
                    <input class="input100" type="text" disabled="disabled" value="@Html.DisplayFor(model => model.Item2.CNPJ)">

                    <label>Data da Inscrição</label>
                    <input class="input100" type="text" disabled="disabled" value="@Html.DisplayFor(model => model.Item2.DataInscricao)">

                    <label>DRT</label>
                    <input class="input100" type="text" disabled="disabled" value="@Html.DisplayFor(model => model.Item2.DRT)">

                    <label>Formas de Atuação</label>
                    <input class="input100" type="text" disabled="disabled" value="@Html.DisplayFor(model => model.Item2.FormasAtuacao)">

                    <label>IE</label>
                    <input class="input100" type="text" disabled="disabled" value="@Html.DisplayFor(model => model.Item2.IE)">

                    <label>Nire</label>
                    <input class="input100" type="text" disabled="disabled" value="@Html.DisplayFor(model => model.Item2.Nire)">

                    <label>Nome Empresarial</label>
                    <input class="input100" type="text" disabled="disabled" value="@Html.DisplayFor(model => model.Item2.NomeEmpresarial)">

                    <label>Ocorrencia Fiscal</label>
                    <input class="input100" type="text" disabled="disabled" value="@Html.DisplayFor(model => model.Item2.OcorrenciaFiscal)">

                    <label>Posto Fiscal</label>
                    <input class="input100" type="text" disabled="disabled" value="@Html.DisplayFor(model => model.Item2.PostoFiscal)">

                    <label>Regime Estadual</label>
                    <input class="input100" type="text" disabled="disabled" value="@Html.DisplayFor(model => model.Item2.RegimeEstadual)">

                    <label>Situação</label>
                    <input class="input100" type="text" disabled="disabled" value="@Html.DisplayFor(model => model.Item2.Situacao)">

                    <label>Tipo da Unidade</label>
                    <input class="input100" type="text" disabled="disabled" value="@Html.DisplayFor(model => model.Item2.TipoUnidade)">

                    <br />

                    <span class="login100-form-title p-b-34 p-t-27">
                        Jucesp
                    </span>

                    <label>Data</label>
                    <input class="input100" type="text" disabled="disabled" value="@Html.DisplayFor(model => model.Item3.Data)">

                    <label>Nome</label>
                    <input class="input100" type="text" disabled="disabled" value="@Html.DisplayFor(model => model.Item3.Nome)">

